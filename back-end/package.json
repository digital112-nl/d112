{
  "name": "d112-back-end",
  "version": "1.0.0",
  "description": "Here an example project with Mongoose and Ts.ED framework.",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "yarn tsc",
    "test": "yarn clean && yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "docker:build": "yarn build && docker-compose build",
    "deploy": "exit 0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/common": "5.65.1",
    "@tsed/core": "5.65.1",
    "@tsed/di": "5.65.1",
    "@tsed/exceptions": "5.65.1",
    "@tsed/mongoose": "5.65.1",
    "@tsed/platform-express": "5.65.1",
    "@tsed/socketio": "5.65.1",
    "@tsed/swagger": "5.65.1",
    "@tsed/testing-mongoose": "5.65.1",
    "@types/swagger-schema-official": "2.0.21",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "^2.8.5",
    "cross-env": "7.0.2",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "generator-utils": "^2.1.0",
    "lodash": "^4.17.20",
    "method-override": "^3.0.0",
    "mongoose": "^5.10.4",
    "node-wit": "^6.0.0",
    "serve-static": "^1.13.1",
    "socket.io": "^2.3.0",
    "token-generator": "^1.0.0",
    "twilio": "^3.49.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "4.2.12",
    "@types/chai-as-promised": "7.1.3",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.7",
    "@types/http-proxy": "^1.17.1",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "8.0.1",
    "@types/mongoose": "^5.7.36",
    "@types/node": "14.0.27",
    "@types/request-promise": "^4.1.42",
    "@types/sinon": "9.0.4",
    "@types/sinon-chai": "3.2.4",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "2.0.10",
    "@types/twilio": "^2.11.0",
    "chai": "4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "5.2.0",
    "mocha": "8.1.1",
    "nodemon": "1.19.4",
    "nyc": "15.1.0",
    "rimraf": "^3.0.0",
    "sinon": "9.0.2",
    "sinon-chai": "3.5.0",
    "supertest": "4.0.2",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "typescript": "3.9.4"
  }
